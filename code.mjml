<mjml>
  <mj-head>
    <mj-style>
      @media (max-width:320px) {
      .header-txt {
      padding: 0 50px !important;
      }

      .header-txt div {
      font-size: 35px !important;
      }
      }

      @media (max-width:250px) {
      .header-txt {
      padding: 0 50px !important;
      }

      .header-txt div {
      font-size: 35px !important;
      }
      }
    </mj-style>
  </mj-head>
  <mj-body background-color="#fff">
    <mj-section background-color="#0F0927" padding="30px 0px 0px">
