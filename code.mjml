        <mj-text css-class="header-txt" color="#fff" font-size="38px" padding="0 70px" font-weight="bold" font-family="saintecolombe-regular, Times New Roman, Times, serif">Test <span style="font-family:Helvetica, Arial, sans-serif;">Template</span></mj-text>
        <mj-image width="600px" src="https://i.ibb.co/kM7RRk8/header-top.jpg" alt="" padding="30px 30px 0px" href="" />
        </mj-image>
      </mj-column>
    </mj-section>
    <mj-section padding="0">
      <mj-column>
        <mj-image width="600px" src="https://i.ibb.co/sRD6vs3/header-bottom.jpg" alt="" padding="0 30px" href="" />
        </mj-image>
      </mj-column>
    </mj-section>
    <mj-section padding="20px">
      <mj-column>
        <mj-image width="200px" src="https://i.ibb.co/xCfhtw3/photo-one.jpg" alt="" padding="0 10px 10px" href="" />
        </mj-image>
      </mj-column>
      <mj-column>
        <mj-image width="200px" src="https://i.ibb.co/Pz5WQVV/photo-two.jpg" alt="" padding="0 10px 10px" href="" />
        </mj-image>
      </mj-column>
      <mj-column>
        <mj-image width="200px" src="https://i.ibb.co/yFZ8Wjn/photo-three.jpg" alt="" padding="0 10px 10px" href="" />
        </mj-image>
      </mj-column>
    </mj-section>
  </mj-body>
</mjml>
